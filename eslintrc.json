{
  "extends": ["standard", "plugin:ava/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 7,
    "sourceType": "module",
    "ecmaFeatures": {
      "impliedStrict": true
    }
  },
  "plugins": ["ava", "babel"],
  "rules": {
    "arrow-parens": 0,
    "babel/arrow-parens": [2, "as-needed"]
  }
}
